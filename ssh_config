Host *.production *.vpcproduction *.ops
  StrictHostKeyChecking no
  IdentityFile ~/.ssh/production.pem
  User ubuntu

Host *.staging *.vpcstaging *.development *.upgrade
  StrictHostKeyChecking no
  IdentityFile ~/.ssh/staging.pem
  User ubuntu

Host *
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/id_rsa
